{
  "expo": {
    "name": "Exceptional ALIEN",
    "slug": "exceptional-alien-app",
    "version": "2.0.0",
    "icon": "./assets/icon.png",
    "scheme": "ea-app-scheme",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2220C1"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Exceptional ALIEN to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b8d714e2-e3d7-4787-816a-cc7e47bca4bd"
      }
    }
  }
}
